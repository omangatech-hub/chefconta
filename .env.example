# Configurações do Sistema ChefConta
# Copie este arquivo para .env e configure as variáveis

# Banco de Dados
DATABASE_URL=sqlite:///database/chefconta.db

# Segurança
SECRET_KEY=your-secret-key-here-change-in-production
JWT_SECRET_KEY=your-jwt-secret-key-here

# Empresa
COMPANY_NAME=Minha Empresa
COMPANY_CNPJ=00.000.000/0000-00
COMPANY_ADDRESS=Rua Exemplo, 123

# Sistema
DEBUG_MODE=True
LOG_LEVEL=INFO

# Licenciamento (opcional)
LICENSE_KEY=
LICENSE_ACTIVE=False
